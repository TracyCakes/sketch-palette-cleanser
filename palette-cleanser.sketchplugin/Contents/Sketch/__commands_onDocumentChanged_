function onDocumentChanged(context) {
  var document = Document.getSelectedDocument();
  var changePaths = Array.from(context.actionContext).map(function (actionContext) {
    return actionContext.fullPath().toString().trim();
  });
  var changePath = changePaths[0];
  var changePathSegments = changePath.match(/([a-z])\w+/g);
  var isNewSharedStyle = changePath.includes('layerStyles') && changePathSegments.length === 1;
  var isSharedStyleRename = changePath.includes('layerStyles') && changePathSegments.length === 2;

  if (isNewSharedStyle) {
    changePath = changePaths[1];
    changePathSegments = changePath.match(/([a-z])\w+/g);
  }

  var finalSegment = changePathSegments[changePathSegments.length - 1];
  var actionContext = context.actionContext[0];

