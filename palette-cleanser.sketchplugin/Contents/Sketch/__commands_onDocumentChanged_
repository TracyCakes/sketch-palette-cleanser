function addNewCell(color, type, cellToRemove, document, isNewSharedStyle) {
  var hasCellToRemove = !!cellToRemove;
  var layerStylePrefix = isNewSharedStyle ? 'shared_layer' : 'layer';
  var parentLayer = find("[name=\"".concat(layerStylePrefix, "_").concat(type, "s\"]"))[0];
  var lowestLayerY = parentLayer.layers.map(function (layer) {
    return layer.frame.y;
  }).sort(function (a, b) {
    return b - a;
  })[0];
  var y = lowestLayerY + _utils_constants__WEBPACK_IMPORTED_MODULE_2__["BASE_CELL_HEIGHT"];

  if (hasCellToRemove) {
    y = isNewSharedStyle ? y : cellToRemove.frame.y;
    cleanlyRemoveCell(cellToRemove, type, isNewSharedStyle, document);
  }

  if (!hasCellToRemove || isNewSharedStyle) {
    if (type === 'fill') {
      var layerBorders = find("[name=\"".concat(layerStylePrefix, "_borders\"]"))[0];
      layerBorders.frame.y += _utils_constants__WEBPACK_IMPORTED_MODULE_2__["BASE_CELL_HEIGHT"];
    }

