  if (finalSegment === 'sharedStyleID' || finalSegment === 'layer') {
    Object(_handleLayerAssignment__WEBPACK_IMPORTED_MODULE_2__["default"])(changePath, storedColorPathDict, document, isNewSharedStyle);
    return;
